		<!-- JQuery to show/hide app-install-footer  -->
		<script type="text/javascript">
            $('input[type=text]').focus(
                function () {
                    $('#app-install-wrap').hide();
                }
            );
           if($('#app-install-wrap').css('display') != 'none'){
           $('input[type=text]').blur(
                    function () {
                    	$('#app-install-wrap').show();
                    }
            );
       }
		</script>

