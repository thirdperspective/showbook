			// The function should only be there for in case of showcase
			function fnPushDLFeatEvent(obj) {
				if (!$.isEmptyObject(obj)) {
					BMS.Misc.fnPushEventDataToAnalytics(["GA"], "", {}, { 'event': 'promotionClick', 'ecommerce': { 'promoClick': { 'promotions': [obj] } } });
				}
			}

			//delete the showcase-overlay cookie that was used for salman's banner promo
			if (BMS.Storage.isset({name: 'showcase-overlay'})) {
				BMS.Storage.del({name: 'showcase-overlay'});
			}

			//keeping this later as it has dependency on misc
			if (displayShowCaseOverlay) {
				//set the cookie with an expiry of 24 hours
				var currentDateForCookie = new Date();
	    		currentDateForCookie.setTime(currentDateForCookie.getTime() + (24 * 60 * 60 * 1000));
	    		var cookieExpiresOn = currentDateForCookie.toGMTString();

				BMS.Storage.set({name: 'ET00020361',
					value: true,
					storage: "C",
					sess: false,
					expires: cookieExpiresOn
				});
			}
		</script>
		
