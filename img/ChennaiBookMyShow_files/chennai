		<script async type="text/javascript">
			SeatData             =   null;
			var getSeatScript = '//in.bookmyshow.com/js/seatlayout-325b2d7c1e.js';

			if(global.blnIsTouchScreen){

					console.log("Loaded Now")
				var $btnNS = $('.button-now-showing'),
				 	$btnCS = $('.button-coming-soon'),
				 	$carouselNS = $('.carousel-now-showing'),
					$carouselCS = $('.carousel-coming-soon');

				if ( $('#now-showing-carousel').hasClass('isEmpty') ) {

					$btnCS.addClass('_active');
					$carouselCS.css({'opacity' : 1, 'height' : 'auto'});

				} else {

					$btnNS.click(function () {
						console.log("Test")
						$btnCS.removeClass('_active');
						$(this).addClass('_active');
						$carouselCS.css({'opacity' : 0, 'height' : '0px'});
						$carouselNS.css({'opacity' : 1, 'height' : 'auto'});
						if(wow) {
							wow.scrolled = true;
				    		wow.scrollHandler();
						}
					});

					$btnCS.click(function () {
						console.log("Test 1")
						$btnNS.removeClass('_active');
						$(this).addClass('_active');
						$carouselNS.css({'opacity' : 0, 'height' : '0px'});
						$carouselCS.css({'opacity' : 1, 'height' : 'auto'});

						if(wow) {
							wow.scrolled = true;
				    		wow.scrollHandler();
						}
					});

				}

			}

		</script>
